{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@alexkunin/ngx-misc": {
      "projectType": "library",
      "root": "projects/alexkunin/ngx-misc",
      "sourceRoot": "projects/alexkunin/ngx-misc/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/alexkunin/ngx-misc/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/alexkunin/ngx-misc/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/alexkunin/ngx-misc/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-builders/jest:run"
        }
      }
    }
  },
  "defaultProject": "@alexkunin/ngx-misc"
}
